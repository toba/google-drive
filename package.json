{
   "name": "@toba/google-drive",
   "version": "1.7.0",
   "description": "Toba Google Drive module",
   "private": false,
   "author": {
      "name": "Jason Abbott",
      "url": "http://github.com/toba"
   },
   "license": "MIT",
   "publishConfig": {
      "access": "public"
   },
   "main": "cjs/index.js",
   "module": "esm/index.js",
   "types": "esm/index.d.ts",
   "files": [
      "esm",
      "cjs"
   ],
   "scripts": {
      "build:esm": "./node_modules/.bin/tsc --project ./node_modules/@toba/develop/tsconfig.esm.json",
      "build:cjs": "./node_modules/.bin/tsc --project ./node_modules/@toba/develop/tsconfig.cjs-min.json",
      "prepublishOnly": "npm run build:esm & npm run build:cjs",
      "test": "jest --coverage"
   },
   "//": "lock google-auth-library to the same version imported by googleapis",
   "devDependencies": {
      "@toba/develop": "^6.6.0",
      "@toba/test": "^4.1.0",
      "@types/node": "^12.0.0",
      "google-auth-library": "^2.0.0",
      "typescript": "^3.5.0"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/toba/google-drive"
   },
   "bugs": {
      "url": "https://github.com/toba/google-drive/issues"
   },
   "dependencies": {
      "@toba/node-tools": "^1.4.6",
      "@toba/oauth": "^2.1.0",
      "googleapis": "^34.0.0"
   }
}
